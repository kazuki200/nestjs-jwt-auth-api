model User {
  id         String       @id @default(uuid())
  name       String
  email      String       @unique
  password   String
  verifiedAt DateTime?
  createdAt  DateTime     @default(now())
  updatedAt  DateTime     @updatedAt
  deletedAt  DateTime?
  form       Form[]
  profile    UserProfile?
}

model UserProfile {
  id              String    @id @default(uuid())
  userId          String    @unique
  profileImageUrl String?
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  deletedAt       DateTime?
  user            User      @relation(fields: [userId], references: [id])
}
